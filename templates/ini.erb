[xdebug]
xdebug.ini                    = <%= @ini_file_path %>
xdebug.remote_enable          = <%= @remote_enable %>
xdebug.remote_port            = <%= @remote_port %>
xdebug.remote_host            = <%= @remote_host %>
xdebug.remote_autostart       = <%= @remote_autostart %>
xdebug.remote_connect_back    = <%= @remote_connect_back %>
xdebug.default_enable         = <%= @default_enable %>
xdebug.show_local_vars        = <%= @show_local_vars %>
xdebug.var_display_max_data   = <%= @var_display_max_data %>
xdebug.var_display_max_depth  = <%= @var_display_max_depth %>
xdebug.show_exception_trace   = <%= @show_exception_trace %>
xdebug.max_nesting_level      = <%= @max_nesting_level %>
xdebug.profiler_output_dir    = <%= @profiler_output_dir %>
xdebug.idekey                 = <%= @idekey %>